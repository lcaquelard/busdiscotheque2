{% extends 'base.html.twig' %}
{% block title %}Les bus classiques{% endblock %}
{% block main %}
    <section id="sodade" class="bus rev">
        <div class="front">
            <div class="content">
                <h3>le sodade</h3>
                <div class="bus-icon dj">DJ</div>
                <div class="bus-icon capacity">35 max</div>
                <div class="bus-icon carpet">Tapis rouge</div>
                <div class="bus-icon soft">Softs inclus</div>
                <div class="bus-icon screen">Ecran HD</div>
                <div class="bus-icon fridge">Espace frigorifié</div>
            </div>
        </div>
        <div class="back">
            <div class="content">
                <h3>le sodade</h3>
                <div class="zoom"></div>
            </div>
        </div>
    </section>
    <section id="poowood" class="bus rev">
        <div class="front">
            <div class="content">
                <h3>le poowood</h3>
                <div class="bus-icon dj">DJ</div>
