{% block modals %}
    <div class="modal sodade" role="dialog" aria-modal="true" aria-label="Galerie de photos du Sodade agrandie">
        <div class="close"></div>
        <div class="slider">
            <img src="{{ asset('build/images/photo/bus/classic/sodade/1.jpg') }}" alt="Image défilant horizontalement">
            <img src="{{ asset('build/images/photo/bus/classic/sodade/2.jpg') }}" alt="Image défilant horizontalement">
            <img src="{{ asset('build/images/photo/bus/classic/sodade/3.jpg') }}" alt="Image défilant horizontalement">
        </div>
    </div>
